{
  "patterns": {
    "test:util": {
      "dir": "test/utils",
      "caseStyle": "kebab",
      "template": "templates/utils",
      "replace": [
        {
          "from": "[name]",
          "to": "[name]"
        },
        {
          "from": "_Default_",
          "to": "[name]",
          "caseStyle": "camel"
        }
      ]
    },
    "test:command": {
      "dir": "test/commands",
      "caseStyle": "kebab",
      "template": "templates/test",
      "replace": [
        {
          "from": "[command]",
          "to": "[name]"
        }
      ]
    }
  }
}
