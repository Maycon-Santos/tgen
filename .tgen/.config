{
  "patterns": {
    "component": {
      "dir": "generated_files/components",
      "caseStyle": "kebab",
      "template": "templates/component",
      "replace": [
        {
          "caseStyle": "pascal",
          "regexp": false,
          "from": "Component",
          "to": "[name]"
        },
        {
          "caseStyle": "kebab",
          "from": "[name]",
          "to": "[name]"
        }
      ]
    },
    "test": {
      "dir": "test/commands",
      "caseStyle": "kebab",
      "template": "templates/test",
      "replace": [
        {
          "from": "[command]",
          "to": "[name]"
        }
      ]
    }
  }
}
